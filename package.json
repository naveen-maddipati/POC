{
  "name": "@nuxeo-satori-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build Nuxeo-Satori-UI",
    "start": "nx serve Nuxeo-Satori-UI",
    "pack": "nx pack Nuxeo-Satori-UI",
    "satori": "satori",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "type-check": "npx tsc --project tsconfig.base.json --noEmit",
    "type-check:watch": "npx tsc --project tsconfig.base.json --noEmit --watch",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --code-coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "lint-staged",
    "clean": "nx reset && rm -rf dist node_modules/.cache",
    "clean:dist": "rm -rf dist",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "validate": "npm run lint && npm run type-check && npm run test && npm run build",
    "dev:validate": "npm run lint:fix && npm run type-check",
    "pre-commit": "npm run dev:validate",
    "ci": "npm run validate"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~19.2.14",
    "@angular/compiler": "~19.2.0",
    "@angular/core": "~19.2.14",
    "@angular/forms": "~19.2.0",
    "@angular/platform-browser": "~19.2.14",
    "@angular/platform-browser-dynamic": "~19.2.0",
    "@angular/router": "~19.2.14",
    "@hylandsoftware/satori-devkit": "latest",
    "@hylandsoftware/satori-layout": "^0.1.0",
    "@hylandsoftware/satori-ui": "^0.1.0",
    "@hylandsoftware/satori-workspace": "0.1.0",
    "@ngx-translate/core": "^17.0.0",
    "@types/node": "^24.7.2",
    "bestzip": "^2.2.1",
    "nuxeo": "^4.0.5",
    "oidc-client-ts": "^3.3.0",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~19.2.0",
    "@angular-devkit/core": "~19.2.0",
    "@angular-devkit/schematics": "~19.2.0",
    "@angular/cli": "~19.2.0",
    "@angular/compiler-cli": "~19.2.0",
    "@angular/language-service": "~19.2.0",
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "21.1.3",
    "@nx/eslint-plugin": "21.1.3",
    "@nx/js": "21.1.3",
    "@nx/web": "21.1.3",
    "@nx/workspace": "21.1.3",
    "@schematics/angular": "~19.2.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@typescript-eslint/utils": "^8.19.0",
    "angular-eslint": "^19.2.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "nx": "21.1.3",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
