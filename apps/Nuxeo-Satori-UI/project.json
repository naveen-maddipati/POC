{
  "name": "Nuxeo-Satori-UI",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/Nuxeo-Satori-UI/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/Nuxeo-Satori-UI",
        "index": "apps/Nuxeo-Satori-UI/src/index.html",
        "browser": "apps/Nuxeo-Satori-UI/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/Nuxeo-Satori-UI/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/Nuxeo-Satori-UI/public"
          },
          "apps/Nuxeo-Satori-UI/src/app.config.json",
          {
            "glob": "**/*",
            "input": "node_modules/@hylandsoftware/satori-layout/assets",
            "output": "plugins/@hylandsoftware/satori-layout/assets"
          },
          {
            "glob": "*.json",
            "input": "node_modules/@hylandsoftware/satori-layout/i18n",
            "output": "i18n/@hylandsoftware/satori-layout"
          },
          {
            "glob": "*.json",
            "input": "node_modules/@hylandsoftware/satori-ui/i18n",
            "output": "i18n/@hylandsoftware/satori-ui"
          }
        ],
        "styles": [
          "apps/Nuxeo-Satori-UI/src/styles.scss",
          "node_modules/@hylandsoftware/satori-layout/styles/satori-layout.scss"
        ],
        "scripts": [],
        "baseHref": "./",
        "allowedCommonJsDependencies": [
          "nuxeo"
        ]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "port": 4201,
        "proxyConfig": "proxy.conf.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "Nuxeo-Satori-UI:build:production"
        },
        "development": {
          "buildTarget": "Nuxeo-Satori-UI:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "Nuxeo-Satori-UI:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "Nuxeo-Satori-UI:build",
        "port": 4201,
        "staticFilePath": "dist/apps/Nuxeo-Satori-UI/browser",
        "spa": true
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd dist/apps/Nuxeo-Satori-UI/browser && bestzip ../../Nuxeo-Satori-UI.zip ."
      },
      "dependsOn": ["build"]
    }
  }
}
